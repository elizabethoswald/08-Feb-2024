//Polymorphism
class Car
{
	public void getCost()
	{
		System.out.println("Cars cost from 1 lakh");
	}
}

class Tesla extends Car
{
	public void getCost()
	{
		System.out.println("Tesla costs 40 lakhs");
	}
}
public class Example_For_Polymorphism {
	public static void main(String[] args)
	{
		Car c1=new Car();
		c1.getCost();
		Car c2=new Tesla();
		c2.getCost();
	}
}
